<head>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
    integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
    integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
    crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
    integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
    crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
    integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
    crossorigin="anonymous"></script>
  <style>
    .bs-vertical-slider .carousel-item-next.carousel-item-left,
    .bs-vertical-slider .carousel-item-prev.carousel-item-right {
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }

    .bs-vertical-slider .carousel-item-next,
    .bs-vertical-slider .active.carousel-item-right {
      -webkit-transform: translate3d(0, 100%, 0);
      transform: translate3d(0, 100% 0);
    }

    .bs-vertical-slider .carousel-item-prev,
    .bs-vertical-slider .active.carousel-item-left {
      -webkit-transform: translate3d(0, -100%, 0);
      transform: translate3d(0, -100%, 0);
    }

    #memeholder {
      min-height: 250px;

    }

    .meme {
      max-height: 350px;
      width: auto;
    }

    .center {
      text-align: center;
      margin-top: 1%
    }

    #carousel {
      height: 300px;
    }

    #pong {
      background-image: url("https://i.ytimg.com/vi/e4VRgY3tkh0/hqdefault.jpg");
      height: 100%;
    }
  </style>
</head>

<body>

  <div class="container">
    <div class="row">
      <div class="col">
        <h1>
          <p style="text-align: center">ChatSpace</p>
        </h1>
        <hr>
      </div>
    </div>
    <div class="row" id="middlerow">
      <div class="col">
        <h1>Channels</h1>
        <hr style="border-block-width:">
        <div class="container" id="carousel">
          <div id="bs4-vertical-slide-carousel" class="carousel bs-vertical-slider slide" data-ride="carousel">
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img class="d-block mx-auto img-fluid"
                  src="data:image/png;base64,"
                  alt="Slide Number 1">
              </div>
              <div class="carousel-item">
                <img class="d-block mx-auto img-fluid"
                  src="data:image/jpeg;base64,"
                  alt="Slide Number 2">
              </div>
              <div class="carousel-item">
                <img class="d-block mx-auto img-fluid"
                  src="https://lh3.googleusercontent.com/yAtZnNL-9Eb5VYSsCaOC7KAsOVIJcY8mpKa0MoF-0HCL6b0OrFcBizURHywpuip-D6Y"
                  alt="Slide Number 3">
              </div>
            </div>
          </div>
        </div>

      </div>
      <div class="col">
        <h1>Messages</h1>
        <hr>
        <div id="pong">
          <ul>
            {{#each messages}}
            <button class="channel-button" data-name="{{this}}">{{this}}</button>
            <br>
            <br>
            {{/each}}
          </ul>

          <div class="messages-holder">
            <bold>Example message</bold>
          </div>
          <br>
          <form>
            <input type="text" class="message-input" placeholder="Type something">
            <button type="submit" id="submit-button">Submit</button>
          </form>
        </div>
      </div>
    </div>
    <div class="row" style="height:auto" >
      <div class="col">
        <div class="center">
          <div id="memeholder"></div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <div class="center">
          <button id="meme-button"> Meme Me Bro!!</button>
        </div>
      </div>
    </div>

  </div>
  <script>
    $('#bs4-vertical-slide-carousel').carousel({
      interval: 1000
    })
    $('#btnPrev').on('click', function () {
      $('#bs4-vertical-slide-carousel').carousel('prev');
    })
    $('#btnPause').on('click', function () {
      $('#bs4-vertical-slide-carousel').carousel('pause');
    })
    $('#btnNext').on('click', function () {
      $('#bs4-vertical-slide-carousel').carousel('next');
    })
    // The createRow function takes data returned by OMDB and appends the table data to the tbody
    $("#meme-button").on("click", function (event) {
      event.preventDefault();
      $('#memeholder').empty();
      searchForMeme();
    })
    var searchForMeme = function () {
      var queryURL = "https://api.imgflip.com/get_memes";
      $.ajax({
        url: queryURL,
        method: "GET"
      }).then(function (response) {
        //console.log(response.data.memes[0].url);
        let memeSelect = Math.floor(Math.random() * 101);
        let newImage = $("<img>");
        newImage.attr("src", response.data.memes[memeSelect].url)
        newImage.attr("class", "meme")
        console.log(response.data.memes[memeSelect].url);
        $("#memeholder").prepend(newImage);
      });
    }
  </script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
</body>